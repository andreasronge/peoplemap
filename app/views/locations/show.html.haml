%table{:cellpadding => "0", :cellspacing => "0", :border => "0", :class => "node_info", :width => "100%"}
  %tr
    %td{:width=>"600px", :id=>"node_left_col"}
      = render :partial=>"data"
      %div{:id=>"visualisation"}
        = render :partial=>"/shared/visualisation"
    %td{:id=>"node_linker"}
      = render( :partial => "shared/list_rels", :object => @object)
      = render( :partial => "shared/all_nodes", :object => @object )
